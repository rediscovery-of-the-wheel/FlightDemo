<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  android:orientation="vertical"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent">

	<LinearLayout
		android:orientation="vertical"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true">

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="@string/info_version_app"
			android:id="@+id/textView12"
			android:textSize="@dimen/text_sz_list"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="New Text"
			android:id="@+id/app_version_textview"
			android:textSize="@dimen/text_sz_list"
			android:paddingBottom="@dimen/list_padding"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="@string/info_product_name"
			android:id="@+id/textView13"
			android:textSize="@dimen/text_sz_list"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="New Text"
			android:id="@+id/product_name_textview"
			android:textSize="@dimen/text_sz_list"
			android:paddingBottom="@dimen/list_padding"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="@string/info_version_software"
			android:id="@+id/textView2"
			android:textSize="@dimen/text_sz_list"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="New Text"
			android:id="@+id/software_version_textview"
			android:textSize="@dimen/text_sz_list"
			android:paddingBottom="@dimen/list_padding"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="@string/info_version_hardware"
			android:id="@+id/textView3"
			android:textSize="@dimen/text_sz_list"
			android:textAlignment="center"
			android:gravity="center"/>

		<TextView
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:text="New Text"
			android:id="@+id/hardware_version_textview"
			android:textSize="@dimen/text_sz_list"
			android:paddingBottom="@dimen/list_padding"
			android:textAlignment="center"
			android:gravity="center"/>
	</LinearLayout>

</RelativeLayout>